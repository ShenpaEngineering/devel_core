{
  "version": "0.1.17",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "publish": "tsdx build && cp ./src/dockerfiles/docker-compose.njk ./dist/docker-compose.njk && cp ./src/dockerfiles/dockerfile-node.njk ./dist/dockerfile-node.njk && cp ./src/dockerfiles/Vagrantfile.njk ./dist/Vagrantfile.njk",
    "test": "tsdx test",
    "lint": "yarn lint",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "devtool_lib",
  "author": "Shariq Torres",
  "module": "dist/devtool_lib.esm.js",
  "size-limit": [
    {
      "path": "dist/devtool_lib.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/devtool_lib.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@nano-sql/core": "^2.3.7",
    "@octokit/rest": "^18.12.0",
    "@types/jest": "^27.0.3",
    "@types/mocha": "^9.0.0",
    "@types/nedb": "^1.8.12",
    "@types/node": "^16.11.9",
    "@types/nunjucks": "^3.2.0",
    "adm-zip": "^0.5.9",
    "axios": "^0.24.0",
    "fs-extra": "^10.0.0",
    "mock-fs": "^5.1.2",
    "nedb": "^1.8.0",
    "node-json-db": "^1.4.1",
    "nunjucks": "^3.2.3",
    "requets": "0.0.1-security",
    "tiny-typed-emitter": "^2.1.0",
    "tslib": "^2.3.1"
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/test/*.test.ts"
    ],
    "testTimeout": 30000,
    "maxWorkers": 4
  }
}
